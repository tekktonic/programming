<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.24"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="Toplevel">
    <property name="width_request">800</property>
    <property name="height_request">450</property>
    <property name="can_focus">False</property>
    <signal name="delete-event" handler="Gtk3::main_quit" swapped="no"/>
    <child>
      <object class="GtkNotebook" id="MainView">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="border_width">3</property>
        <property name="tab_border">10</property>
        <property name="tab_hborder">10</property>
        <property name="tab_vborder">10</property>
        <child>
          <object class="GtkButton" id="button1">
            <property name="label" translatable="yes">button</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
          </object>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="label2">
            <property name="sensitive">False</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">label2</property>
          </object>
          <packing>
            <property name="position">2</property>
            <property name="tab_fill">False</property>
          </packing>
        </child>
        <child>
          <placeholder/>
        </child>
        <child type="tab">
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
        <child type="tab">
          <object class="GtkLabel" id="News">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">News</property>
          </object>
          <packing>
            <property name="position">2</property>
            <property name="tab_fill">False</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
